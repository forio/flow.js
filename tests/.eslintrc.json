{
    "extends": ["../.eslintrc.json"],
    "env": {
        "mocha": true,
        "es6": true
    },
    "globals": {
        "expect": false,
        "sinon": false,
        "F": false,
        "$": false,
        "_": false,
        "should": false
    },
    "rules": {
        "no-magic-numbers": 0,
        "one-var": 0,
        "no-unused-expressions": 0
    }
}
